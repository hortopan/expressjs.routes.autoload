{
  "extends": "tslint:latest",
  "rules": {
    "no-console": false,
    "object-literal-sort-keys": false,
    "quotemark": false,
    "max-line-length": [
      true,
      200
    ],
    "typedef": [
      true,
      "call-signature",
      "property-declaration"
    ],
    "no-shadowed-variable": false,
    "prefer-object-spread": false,
    "no-submodule-imports": false,
    "variable-name": {
      "options": [
        "ban-keywords",
        "check-format",
        "allow-pascal-case",
        "allow-leading-underscore",
        "allow-snake-case"
      ]
    }
  },
  "jsRules": {
    "no-console": false,
    "no-shadowed-variable": false,
    "prefer-object-spread": false,
    "no-submodule-imports": false,
    "variable-name": {
      "options": [
        "ban-keywords",
        "check-format",
        "allow-pascal-case",
        "allow-leading-underscore",
        "allow-snake-case"
      ]
    }
  }
}
